{
  "name": "CosyBlog",
  "version": "0.0.1",
  "description": "A VuePress project",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "new": "node ./theme/bin/newDocs.js",
    "dev": "cross-env BASE=/BlogByCosy/ MODE=dev vitepress dev . --no-cache",
    "build": "vitepress build . --no-cache",
    "build:github": "cross-env BASE=/BlogByCosy/ vitepress build .",
    "preview": "vitepress preview ."
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "sass": "^1.89.0",
    "sass-embedded": "^1.89.0",
    "vitepress": "^1.6.3"
  }
}
